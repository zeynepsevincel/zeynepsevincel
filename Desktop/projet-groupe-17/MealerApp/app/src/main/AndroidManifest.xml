<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="engineering.uottawa.mealerapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Connexion" />
        <activity android:name=".ConnexionAdmin" />
        <activity android:name=".Admin" />
        <activity android:name=".ConnexionClient" />
        <activity android:name=".Client" />
        <activity android:name=".ConnexionCuisinier" />
        <activity android:name=".Cuisinier" />
        <activity android:name=".CreationClient" />
        <activity android:name=".CreationCuisinier" />
        <activity android:name=".Creation"></activity>
    </application>

</manifest>